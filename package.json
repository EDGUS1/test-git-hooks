{
  "name": "test-git-hooks",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "release": "standard-version",
    "test": "jest",
    "prepare": "husky install",
    "hook": "husky add .husky/commit-msg \"commitlint -E  HUSKY_GIT_PARAMS\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EDGUS1/test-git-hooks.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/EDGUS1/test-git-hooks/issues"
  },
  "homepage": "https://github.com/EDGUS1/test-git-hooks#readme",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "standard-version": "^9.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test",
      "commit-msg": "commitlint -E  HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
